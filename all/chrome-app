#!/bin/bash

EXEC=$(basename $0)
shift

case $EXEC in
	gmusic)  URL=https://play.google.com/music/listen ;;
	ingress) URL=https://ingress.com/intel ;;
	meeting) URL=https://meet.google.com/?authuser=1 ;;
	netflix) URL=https://netflix.com/browse ;;
	youtube) URL=https://youtube.com ;;
	*) echo "What?" && exit 1 ;;
esac

google-chrome --app=$URL
